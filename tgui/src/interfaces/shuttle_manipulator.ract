<link rel='ractive' href='./shuttle_manipulator/status.ract'>

<status></status>
<ui-tabs tabs='{{data.tabs}}'>
	{{#each data.shuttles}}
		<tab name='{{port_id}}'>
			{{#each templates}}
				<ui-display title='{{name}}'>
					{{#if description}}
						<ui-section label='Description'>{{description}}</ui-section>
					{{/if}}
					{{#if admin_notes}}
						<ui-section label='Admin Notes'>{{admin_notes}}</ui-section>
					{{/if}}
				</ui-display>
			{{/each}}
		</tab>
	{{/each}}
</ui-tabs>
