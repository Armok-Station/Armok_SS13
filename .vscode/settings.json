{
	"eslint.workingDirectories": [
		"./tgui"
	],

	"workbench.editorAssociations": [
		{
			"filenamePattern": "*.dmi",
			"viewType": "imagePreview.previewEditor"
		}
	],
	"files.eol": "\n",
	"search.exclude": {
		"_maps/**/*": true,
		"*.dme": true,
		"html/archivedchangelog.html": true,
		"html/changelog.html": true,
		"html/changelogs/*": true
	}
}
